    "@types/node": "^20.4.1",
    "@types/react": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-plugin-react": "^7.33.1",
    "node-forge": "^1.3.1",
    "pcd-country-identity": "^0.0.5",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.5",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
